{
  "name": "cryptochain",
  "version": "1.0.0",
  "description": "Sample crypto with blockchain project",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll",
    "lint": "tslint --project tsconfig.json",
    "prettier:base": "prettier --parser typescript",
    "prettier:check": "yarn prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "yarn prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "dev": "yarn start-redis && nodemon src/index.ts",
    "dev-peer": "cross-env GENERATE_PEER_PORT='true' nodemon src/index.ts",
    "start": "nodemon index.ts",
    "start-redis": "redis-server --daemonize yes",
    "pubsub": "nodemon src/pubsub.ts"
  },
  "keywords": [
    "Crypto",
    "Blockchain"
  ],
  "author": "Abhishek Sett <aabhishek.sett@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/redis": "^4.0.11",
    "@types/request": "^2.48.8",
    "cross-env": "^7.0.3",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "express": "^4.17.2",
    "hex-to-bin": "^1.0.1",
    "redis": "2.8.0",
    "request": "^2.88.2"
  }
}
